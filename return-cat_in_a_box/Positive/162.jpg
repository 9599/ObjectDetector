<script type="text/javascript">
	var page_title = {
			nl: 'Pagina niet gevonden',
			fr: 'La page n\'a pas été trouvée'
		},
		lang, homepageUrl;

	lang = getLanguage();
	homepageUrl = getHomePageUrl();

	function getCookie(c_name){
		if (document.cookie.length>0){
			c_start=document.cookie.indexOf(c_name + "=");
			if (c_start!=-1){
				c_start=c_start + c_name.length+1;
				c_end=document.cookie.indexOf(";",c_start);
				if (c_end==-1) c_end=document.cookie.length;
				return unescape(document.cookie.substring(c_start,c_end));
			}
		}
		return false;
	}

	function getLanguage() {
		//The first thing to do for trying to tell user language is lookup the cookie
		var lang = getCookie("lang");
		if (!lang){
			//cookie not found so next thing in logic is to get language from subdomain
			var language_domain = location.pathname.split("/")[1];
			lang = language_domain;
		}

		if (lang !== 'fr') {
			lang = 'nl';	//last step is to set the default lang
		}

		return lang ;
	}

    function getHomePageUrl() {
        var url = document.location.href.split('/');
        return url[0] + '/' + url[1] + '/' + url[2] + '/' + lang + '/';
    }

	function multilanguageMessages(){
		var title = page_title[lang];

		document.title = title;				//Building error page title
		document.getElementById("message_1").innerHTML=title;
		displayElementOneLanguage('main_message_', lang);
		var home_page_url = "brand_homepage_url_"+lang;
		document.getElementById(home_page_url).href=homepageUrl;
		displayElementOneLanguage('lang_', lang);
		displayElementOneLanguage('nav_', lang);
		displayElementOneLanguage('footer_', lang);
		displayElementOneLanguage('change_language_', lang);
		displayElementOneLanguage('ai_', lang);
	}

	function displayElementOneLanguage(element, language) {

		if (typeof element == 'undefined') {
			return;
		};

		var other_language = 'fr';

		if (language == 'fr') {
			other_language = 'nl';
		};

		if (document.getElementById(element + language) != null) {
			document.getElementById(element + language).style.display = 'block';
		}
		if (document.getElementById(element + other_language) != null) {
			document.getElementById(element + other_language).style.display = 'none';
		}
	}

	function randomNumber() {
		var number = Math.random().toString();
		return number.substring(2, number.length);
	}

	Onload(multilanguageMessages);
</script>
