(function() {
  var arrAnswerBlocks=document.getElementsByClassName('answer');

  for (var i=0;i<arrAnswerBlocks.length;i++) {
    var element = arrAnswerBlocks[i];
    var state = {id:(element.id), collapsed:true}; 
