    <script type="text/javascript">
        $(function () {
            DisplayGeolocate('0', '0', '0', 'Global', 0, '', '0', '')
            var iOS = (navigator.userAgent.match(/(iPad|iPhone|iPod|Android)/g) ? true : false);
            var isDev = (location + '').indexOf('localhost') == -1
            var method = geoip2.city;
                    if ((location + '').indexOf('localhost') == -1) {
                        
                        method = geoip2.cityISPOrg;  
                    }
            var lastResort = function () {
                
                $.post('/BMC.Geolocation/Geolocate/SetLocation', {
                        LocationId : 0,
                        MarketAreaName : 'Global',
                        PostalCode : '0'
                    }, function (result) {
                        
                    })
                
                $('#geolocate-needs-postal-code').trigger('click');
            }
            if (!iOS)
            {
                $(function () {
                    method(function (result) {
                        if (!!result && !!result.location && !!result.location.latitude && !!result.location.longitude) {

                            // get location via IP
                            SwitchArea('', function () { }, result.location.latitude, result.location.longitude);
                        }
                        else {
                            lastResort();
                        }
                    }, function () {
                        lastResort();
                    }, {
                        
                        w3c_geolocation_disabled: isDev
                    })
                })
            }
            else
            {
                if (!!navigator.geolocation.getCurrentPosition)
                {
                    navigator.geolocation.getCurrentPosition(function (result) {
                        SwitchArea('', function () { }, result.coords.latitude, result.coords.longitude);
                    },
                    lastResort())
                }
                else
                {
                    lastResort();
                }
            }
            
        });
    </script>
<script type="text/javascript">

    /*
    Disable area link if we are Global.
    */
    $(function () {
        $(document).on('click', '.ServiceArea .area', function (e) {
            if ($(this).attr('href') == '#') {
                e.preventDefault();
            }
        });
    });

    function DisplayGeolocate(postalCode, latitude, longitude, marketAreaName, locationId, locationName, locationCount, areaUrl) {
        if (!!marketAreaName) {
            $('.ServiceArea .area').text(marketAreaName);
            $('.footerSA span').text(marketAreaName);
        }

        if (!!areaUrl) {
            $('.ServiceArea a.area').attr('href', '/' + areaUrl);
            $('a.area2').attr('href', '/' + areaUrl);
        }

        if (postalCode == '0') {
            $('.ServiceArea .area').css('cursor', 'default');
            $('.ServiceArea .area2').hide().prevAll('br:first').hide();
        }
        else {
            $('.ServiceArea .area').css('pointer', '');
            $('.ServiceArea .area2').show().prevAll('br:first').show();
        }

        if (!!locationCount) {
            if (!!locationCount || locationCount == '0') {
                locationCount = parseInt(locationCount);
            }
            else {
                locationCount = '';
            }

            $('.ServiceArea .locations span').text(locationCount);
        }

        if (!!locationName) {

        }
    }

    function SwitchArea(postalCode, callback, latitude, longitude) {
        if ($.trim(postalCode) == '00000') {
            postalCode = '0';
        }
        $.geolocate({
            postalCode: postalCode,
            baseUrl: '/',
            latitude: latitude,
            longitude: longitude,
            success: function (obj) {
                if (!!obj) {
                    $.post('/BMC.Geolocation/Geolocate/SetLocation', obj, function (result) {
                        if (!!result) {
                            DisplayGeolocate(result.PostalCode, result.Latitude, result.Longitude, result.MarketAreaName, result.LocationId, result.LocationName, result.MarketLocationCount, result.AreaUrl);
                        }
                        $.fancybox.close();
                        if (!!result && !!result.LocationId) {

var refresh = location.pathname.startsWith('/products') || location.pathname.startsWith('/services');

if (refresh) { document.location.reload(); }
                        }
                    });
                }
            },
            failure: function () {
                callback();
            }
        });
    }
