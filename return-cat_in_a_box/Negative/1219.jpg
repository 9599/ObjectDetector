<script>
$(document).ready(function(){
	ht = $(".content-wrap").height();
	wd = $(window).width();
	$('#orangeblock').height(ht + 198);
	$('#orangeblock').width(wd-1082);
	
	ht = $(window).height();
	if (ht < 590) $("#navwrapper").css('height',ht);


	$(window).resize(function(){
		ht = $(window).height();
		if (ht < 590) $("#navwrapper").css('height',ht);
	});	

     $(".beforeimage").hover(function(){
          $(this).prepend('<div class="before-overlay">&nbsp;</div>');
          $(this).find('.before-overlay').html('<img class="before-herohover" src="resources/before.png">');
          $(this).find(".before-overlay").width($(this).children('img').width() + 16);
          $(this).find(".before-overlay").height($(this).children('img').height() + 16);
     },function(){
          $('.before-overlay').remove();
          $('.before-herohover').remove();
     }).click(function(){
          return false;
     });
     
	$("#nav ul").hide();
	
	$("a[href='Images/FullPhotos/PortRetainingWall.jpg']").parents('li').each(function(){
		$(this).children('a:first').addClass('active');
		$(this).parent('ul').show();			
	});
	
	$("a[href='Images/FullPhotos/PortRetainingWall.jpg']").siblings('ul').show();

	
	
	$("a[href='Images/FullPhotos/PortRetainingWall.jpg']").addClass('active');
	
	$("#nav a").click(function(e){
		e.stopPropagation();
      	e.preventDefault();
      	window.scrollTo(0,0);
      	LoadNewContent($(this).attr('href'));
      	$("#nav .active").removeClass('active');
      	$(this).addClass('active');

      $(this).parents('li').each(function(){
      	$(this).children('a').addClass('active');
      	$(this).children('ul').slideDown();
      });
     $("#nav ul").hide();
		$(this).parents('li').each(function(){
			$(this).children('a:first').addClass('active');
			$(this).parent('ul').show();	
			$(this).children('ul').show();		
		});      
  


      
   });		
});
</script>
